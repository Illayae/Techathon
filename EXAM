    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        try{String a,b,c,d,e;
          
             Class.forName("com.mysql.cj.jdbc.Driver");
          Connection  con=DriverManager.getConnection("jdbc:mysql://localhost:3306/acad?zeroDateTimeBehavior=CONVERT_TO_NULL","root","mysql");
            Statement st=con.createStatement();   
            a="insert into qosin values('"+ TF1.getText()+"');";
             b="insert into qosin values('"+ TF2.getText()+"');"; 
             c="insert into qosin values('"+ TF3.getText()+"');";
              d="insert into qosin values('"+ TF4.getText()+"');";
               e="insert into qosin values('"+ TF5.getText()+"');";
               st.executeUpdate(a);
               st.executeUpdate(b);
               st.executeUpdate(c);
               st.executeUpdate(d);
               st.executeUpdate(e);
                JOptionPane.showMessageDialog(this,"successfully added");
               
             
             
            
        }
        catch(Exception e){
             JOptionPane.showMessageDialog(this,"Invalid Input");
        }
    }                                        

